{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "ES5",
		"emitDecoratorMetadata": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"outDir": "./build",
		"rootDir": "./",
		"baseUrl": "."
	},
	"exclude": ["**/secrets/**/*.ts"]
}

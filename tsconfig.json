{
	"compilerOptions": {
		"experimentalDecorators": true,
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"outDir": "./build",
		"rootDir": "./",
		"baseUrl": "."
	},
	"exclude": ["**/secrets/**/*.ts"]
}
